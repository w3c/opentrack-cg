###  http://purl.org/athletics#attemptNumber
:attemptNumber rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:integer ;
               rdfs:isDefinedBy <http://purl.org/athletics#> ;
               rdfs:label "attempt number"@en ;
               skos:definition "Integer indicating the correlative number of an athlete's attempts in a round of trials."@en .


###  http://purl.org/athletics#bibIdentifier
:bibIdentifier rdf:type owl:DatatypeProperty ;
               rdfs:domain :CompetitionAction ;
               rdfs:range rdfs:Literal ;
               rdfs:isDefinedBy <http://purl.org/athletics#> ;
               rdfs:label "bib identifier"@en ;
               skos:definition "Text or number identifying the competitor, printed on competitor's bib."@en .


###  http://purl.org/athletics#byPerformance
:byPerformance rdf:type owl:DatatypeProperty ;
               rdfs:domain :QualificationCriteria ;
               rdfs:range xsd:integer ;
               rdfs:isDefinedBy <http://purl.org/athletics#> ;
               rdfs:label "by performance"@en ;
               skos:definition "Number of competitors that are qualified in a round by best performance."@en .


###  http://purl.org/athletics#byPlaceOrStandard
:byPlaceOrStandard rdf:type owl:DatatypeProperty ;
                   rdfs:domain :QualificationCriteria ;
                   rdfs:range xsd:integer ;
                   rdfs:isDefinedBy <http://purl.org/athletics#> ;
                   rdfs:label "by place or standard"@en ;
                   skos:definition "Number of competitors that are qualified in a round by rank (track events) or standard (field events)."@en .


###  http://purl.org/athletics#disqualificationReason
:disqualificationReason rdf:type owl:DatatypeProperty ;
                        rdfs:range rdfs:Literal .


###  http://purl.org/athletics#entryRequirements
:entryRequirements rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Competition ;
                   rdfs:range rdfs:Literal ;
                   rdfs:isDefinedBy <http://purl.org/athletics#> ;
                   rdfs:label "entry requirements"@en ;
                   skos:definition "Requirements to register in the competition."@en .


###  http://purl.org/athletics#increasingHeight
:increasingHeight rdf:type owl:DatatypeProperty .


###  http://purl.org/athletics#isSubstituteTrial
:isSubstituteTrial rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:boolean ;
                   rdfs:isDefinedBy <http://purl.org/athletics#> ;
                   rdfs:label "is substitute trial"@en ;
                   skos:definition "Flag indicating if the trial is a substitute trial."@en .


###  http://purl.org/athletics#points
:points rdf:type owl:DatatypeProperty ;
        rdfs:domain :PointsPerformance ;
        rdfs:range xsd:decimal ;
        rdfs:isDefinedBy <http://purl.org/athletics#/> ;
        rdfs:label "score points"@en ;
        skos:definition "Score points achieved in a sports competition."@en .


###  http://purl.org/athletics#previousFamilyName
:previousFamilyName rdf:type owl:DatatypeProperty ;
                    rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                    rdfs:range rdfs:Literal ;
                    rdfs:isDefinedBy <http://purl.org/athletics#> ;
                    rdfs:label "previous family name"@en ;
                    skos:definition "Previous family name of a person."@en .


###  http://purl.org/athletics#previousGivenName
:previousGivenName rdf:type owl:DatatypeProperty ;
                   rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                   rdfs:range rdfs:Literal ;
                   rdfs:isDefinedBy <http://purl.org/athletics#> ;
                   rdfs:label "previous given name"@en ;
                   skos:definition "Previous given name of a person."@en .


###  http://purl.org/athletics#reactionTime
:reactionTime rdf:type owl:DatatypeProperty ;
              rdfs:domain :TimePerformance ;
              rdfs:range xsd:time ;
              rdfs:isDefinedBy <http://purl.org/athletics#> ;
              rdfs:label "reaction time"@en ;
              skos:definition "Reaction time is the interval time it takes a runner to respond to the start signal and begin leaving the starting blocks. Reaction time should be expressed as ISO 8601 duration format (e.g., 00:00:00.232 for 232 milliseconds)."@en .


###  http://purl.org/athletics#startingOrder
:startingOrder rdf:type owl:DatatypeProperty ;
               rdfs:domain :Result ;
               rdfs:range xsd:integer ;
               rdfs:isDefinedBy <http://purl.org/athletics#> ;
               rdfs:label "starting order"@en ;
               skos:definition "Competitor's order in the start list."@en .


###  http://purl.org/athletics#time
:time rdf:type owl:DatatypeProperty ;
      rdfs:domain :TimePerformance ;
      rdfs:range xsd:time ;
      rdfs:isDefinedBy <http://purl.org/athletics#> ;
      rdfs:label "time"@en ;
      skos:definition "Performance accomplished in a timed competition, expressed as ISO 8601 duration format (e.g., 04:45:38.000 for 4 hours, 45 minutes and 38.000 seconds)."@en .


###  http://purl.org/athletics#trackLane
:trackLane rdf:type owl:DatatypeProperty ;
           rdfs:domain :Result ;
           rdfs:range rdfs:Literal ;
           rdfs:isDefinedBy <http://purl.org/athletics#> ;
           rdfs:label "track lane"@en ;
           skos:definition "Track lane number assigned to a competitor in case of certain track disciplines."@en .


###  http://purl.org/athletics#transponderIdentifier
:transponderIdentifier rdf:type owl:DatatypeProperty ;
                       rdfs:domain :CompetitionAction ;
                       rdfs:range rdfs:Literal ;
                       rdfs:isDefinedBy <http://purl.org/athletics#/> ;
                       rdfs:label "transponder identifier"@en ;
                       skos:definition "Text or code identifying the competitor by a transponder system."@en .


###  http://purl.org/dc/terms/created
dct:created rdf:type owl:DatatypeProperty .


###  http://purl.org/dc/terms/modified
dct:modified rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://purl.org/athletics#Athlete
:Athlete rdf:type owl:Class ;
         rdfs:subClassOf schema:Person ,
                         <http://xmlns.com/foaf/0.1/Person> ;
         rdfs:isDefinedBy <http://purl.org/athletics#> ;
         rdfs:label "Athlete"@en ;
         skos:definition "An Athlete is a Person who participates in Athletics events."@en .


###  http://purl.org/athletics#AthleticsDiscipline
:AthleticsDiscipline rdf:type owl:Class ;
                     rdfs:subClassOf :SportsDiscipline ;
                     rdfs:isDefinedBy <http://purl.org/athletics#> ;
                     rdfs:label "Athletics Discipline"@en ;
                     skos:definition "Athletics discipline is a particular Athletics type of event practiced under specific rules."@en .


###  http://purl.org/athletics#CombinedEvent
:CombinedEvent rdf:type owl:Class ;
               rdfs:subClassOf :AthleticsDiscipline ;
               rdfs:isDefinedBy <http://purl.org/athletics#> ;
               rdfs:label "Combined Event"@en ;
               skos:broader :AthleticsDiscipline ;
               skos:definition "Combined Events are competitions in which athletes participate in a number of track and field events, earning points for their performance in each event, which adds to a total points score."@en ;
               skos:narrower :Decathlon ,
                             :Heptathlon ,
                             :Pentathlon ,
                             :ThrowsPentathlon .


###  http://purl.org/athletics#Competition
:Competition rdf:type owl:Class ;
             rdfs:subClassOf schema:SportsEvent ;
             dct:description "A Competition, Athletics Competition or Sports Competition is an event in which Athletes or Teams take part in order to find out who is best at a particular sports activity."@en ;
             rdfs:isDefinedBy <http://purl.org/athletics#> ;
             rdfs:label "Competition"@en .


###  http://purl.org/athletics#CompetitionAction
:CompetitionAction rdf:type owl:Class ;
                   rdfs:subClassOf schema:ExerciseAction ;
                   rdfs:isDefinedBy <http://purl.org/athletics#> ;
                   rdfs:label "Competition Action"@en ;
                   skos:definition "Competitor or entry in a competition. Athlete or team registered and taking part in a competition. "@en .


###  http://purl.org/athletics#CompetitionRound
:CompetitionRound rdf:type owl:Class ;
                  rdfs:subClassOf :Competition ;
                  dct:description "A Competition Round is each sub-event of a Multi Round Competition. Rounds in competitions have one of more Unit Competitions (e.g., a heat in a qualifying round, the race in the 200m final round) where competitors are distributed. The number and type of rounds depend on the competition rules for each discipline. For instance, track sprint competitions with many participants may have various heats at preliminary round, several heats at first round, two semifinals, and a final. Competition rounds aims at qualifying athletes to next round until the final. There are competitions that only have one final round such as Marathon or Cross Country races. Examples of Competition Rounds: 110m Hurdles Men Preliminary Round Heat 1, 10,000m Men Final, and 110 Hurdles Man Semifinal 1."@en ;
                  rdfs:isDefinedBy <http://purl.org/athletics#> ;
                  rdfs:label "Competition Round"@en .


###  http://purl.org/athletics#CompetitiveAudience
:CompetitiveAudience rdf:type owl:Class ;
                     rdfs:subClassOf schema:PeopleAudience ;
                     rdfs:isDefinedBy <http://purl.org/athletics#> ;
                     rdfs:label "Competitive Audience"@en ;
                     skos:definition "Competitive Audience is the potential audience to take part in the competition of athletes and teams in sports events. This marks the categories of the competition."@en .


###  http://purl.org/athletics#CrossCountry
:CrossCountry rdf:type owl:Class ;
              rdfs:subClassOf :Race ;
              rdfs:isDefinedBy <http://purl.org/athletics#> ;
              rdfs:label "Cross Country"@en ;
              skos:broader :Race ;
              skos:definition "Cross country is a running discipline in which teams and individuals run a race on open-air courses over natural terrain such as dirt or grass."@en .


###  http://purl.org/athletics#Decathlon
:Decathlon rdf:type owl:Class ;
           rdfs:subClassOf :CombinedEvent ;
           rdfs:isDefinedBy <http://purl.org/athletics#> ;
           rdfs:label "Decathlon"@en ;
           skos:broader :CombinedEvent ;
           skos:definition "Decathlon is a competition in which athletes participate in ten track and field combined events, earning points for their performance in each event, which adds to a total points score."@en .


###  http://purl.org/athletics#DiscusThrow
:DiscusThrow rdf:type owl:Class ;
             rdfs:subClassOf :Throws ;
             rdfs:isDefinedBy <http://purl.org/athletics#> ;
             rdfs:label "Discus Throw"@en ;
             skos:broader :Throws ;
             skos:definition "The Discus Throw is a track and field event in which an athlete throws a heavy disc (the discus) in an attempt to mark a farther distance than their competitors. The diameter and weight of the discus varies depending on the competition rules."@en .


###  http://purl.org/athletics#DistancePerformance
:DistancePerformance rdf:type owl:Class ;
                     rdfs:subClassOf :Performance ;
                     rdfs:isDefinedBy <http://purl.org/athletics#> ;
                     rdfs:label "Distance Performance"@en ;
                     skos:definition "Performance measured as distance"@en .


###  http://purl.org/athletics#HammerThrow
:HammerThrow rdf:type owl:Class ;
             rdfs:subClassOf :Throws ;
             rdfs:isDefinedBy <http://purl.org/athletics#> ;
             rdfs:label "Hammer Throw"@en ;
             skos:broader :Throws ;
             skos:definition "Hammer Throw is a track and field event in which an athlete throws a metal ball attached by a steel wire to a grip (the hammer). The size of the ball, and the length of the wire varies depending on the competition rules."@en .


###  http://purl.org/athletics#Heptathlon
:Heptathlon rdf:type owl:Class ;
            rdfs:subClassOf :CombinedEvent ;
            rdfs:isDefinedBy <http://purl.org/athletics#> ;
            rdfs:label "Heptathlon"@en ;
            skos:broader :CombinedEvent ;
            skos:definition "Heptathlon is a competition in which athletes participate in seven track and field combined events, earning points for their performance in each event, which adds to a total points score. "@en .


###  http://purl.org/athletics#HighJump
:HighJump rdf:type owl:Class ;
          rdfs:subClassOf :VerticalJumps ;
          rdfs:isDefinedBy <http://purl.org/athletics#> ;
          rdfs:label "High Jump"@en ;
          skos:broader :VerticalJumps ;
          skos:definition "High Jump is a track and field event in which competitors must jump unaided over a horizontal bar placed at measured heights without dislodging it."@en .


###  http://purl.org/athletics#HorizontalJumps
:HorizontalJumps rdf:type owl:Class ;
                 rdfs:subClassOf :AthleticsDiscipline ;
                 rdfs:isDefinedBy <http://purl.org/athletics#> ;
                 rdfs:label "Horizontal Jumps"@en ;
                 skos:broader :AthleticsDiscipline ;
                 skos:definition "Horizontal Jumps are track and field events in which athletes combine speed, strength and agility in an attempt to leap as far as possible from a take off point. Horizontal jumps include: long jump and triple jump."@en ;
                 skos:narrower :LongJump ,
                               :TripleJump .


###  http://purl.org/athletics#Hurdles
:Hurdles rdf:type owl:Class ;
         rdfs:subClassOf :Race ;
         rdfs:isDefinedBy <http://purl.org/athletics#> ;
         rdfs:label "Hurdles"@en ;
         skos:broader :Race ;
         skos:definition "Hurdles is a running event where athletes jump over hurdles at speed."@en .


###  http://purl.org/athletics#JavelinThrow
:JavelinThrow rdf:type owl:Class ;
              rdfs:subClassOf :Throws ;
              rdfs:isDefinedBy <http://purl.org/athletics#> ;
              rdfs:label "Javelin Throw"@en ;
              skos:broader :Throws ;
              skos:definition "The Javelin Throw is a track and field event where the javelin, a spear about 2.5 m (8 ft 2 in) in length, is thrown. Lenght and weight of javelin may vary depending on the rules of the competition."@en .


###  http://purl.org/athletics#LongJump
:LongJump rdf:type owl:Class ;
          rdfs:subClassOf :HorizontalJumps ;
          rdfs:isDefinedBy <http://purl.org/athletics#> ;
          rdfs:label "Long Jump"@en ;
          skos:definition "Long Jump (or broad jump) is a track and field event in which athletes combine speed, strength and agility in an attempt to leap as far as possible from a take off point."@en .


###  http://purl.org/athletics#MiddleOrLongDistanceRace
:MiddleOrLongDistanceRace rdf:type owl:Class ;
                          rdfs:subClassOf :Race ;
                          rdfs:isDefinedBy <http://purl.org/athletics#> ;
                          rdfs:label "Middle Or Long Distance Race"@en ;
                          skos:broader :Race ;
                          skos:definition "Middle-distance or Long-distance running event, over a distance longer than sprints (over 800m and up to Marathon 42.195 Km)."@en ;
                          skos:narrower :Steeplechase .


###  http://purl.org/athletics#MountainRunning
:MountainRunning rdf:type owl:Class ;
                 rdfs:subClassOf :Race ;
                 rdfs:isDefinedBy <http://purl.org/athletics#> ;
                 rdfs:label "Mountain Running"@en ;
                 skos:broader :Race ;
                 skos:definition "Mountain running is a Athletics discipline that takes place on terrain that is mainly off-road and which course may gain elevation."@en .


###  http://purl.org/athletics#Pentathlon
:Pentathlon rdf:type owl:Class ;
            rdfs:subClassOf :CombinedEvent ;
            rdfs:isDefinedBy <http://purl.org/athletics#> ;
            rdfs:label "Pentathlon"@en ;
            skos:broader :CombinedEvent ;
            skos:definition "Pentathlon is a competition in which athletes participate in five track and field combined events, earning points for their performance in each event, which adds to a total points score."@en .


###  http://purl.org/athletics#Performance
:Performance rdf:type owl:Class ;
             rdfs:isDefinedBy <http://purl.org/athletics#> ;
             rdfs:label "Performance"@en ;
             skos:definition "Performance represents the resulting competitor’s accomplishment measured and recognized by officials after a competition. Measurements depend on the type of discipline (i.e., running performances are measured as time, jumps and throws are measured in centimetres). It may include information about the conditions in which competitor got the performance (e.g., wind assistance)."@en .


###  http://purl.org/athletics#PointsPerformance
:PointsPerformance rdf:type owl:Class ;
                   rdfs:subClassOf :Performance ;
                   rdfs:isDefinedBy <http://purl.org/athletics#> ;
                   rdfs:label "Points Performance"@en ;
                   skos:definition "Performance measured as points"@en .


###  http://purl.org/athletics#PoleVault
:PoleVault rdf:type owl:Class ;
           rdfs:subClassOf :VerticalJumps ;
           rdfs:isDefinedBy <http://purl.org/athletics#> ;
           rdfs:label "Pole Vault"@en ;
           skos:broader :VerticalJumps ;
           skos:definition "Pole vault is a track and field event in which a person uses a long, flexible pole as an aid to jump over a bar."@en .


###  http://purl.org/athletics#QualificationCriteria
:QualificationCriteria rdf:type owl:Class ;
                       rdfs:isDefinedBy <http://purl.org/athletics#> ;
                       rdfs:label "Qualification Criteria"@en ;
                       skos:definition "Qualification Criteria are the requirements for the competitor to pass a competition round. Qualification may be based on the finishing position or by best time."@en .


###  http://purl.org/athletics#Race
:Race rdf:type owl:Class ;
      rdfs:subClassOf :AthleticsDiscipline ;
      rdfs:isDefinedBy <http://purl.org/athletics#> ;
      rdfs:label "Race"@en ;
      skos:broader :AthleticsDiscipline ;
      skos:definition "A type of competition to see who is the fastest, including running or race-waking."@en ;
      skos:narrower :Hurdles ,
                    :MiddleOrLongDistanceRace ,
                    :MountainRunning ,
                    :RaceWalking ,
                    :RoadRunning ,
                    :Steeplechase ,
                    :TrackRelays ,
                    :UltraRunning ,
                    <http://purl.org/athletics#/Sprint> .


###  http://purl.org/athletics#RaceWalking
:RaceWalking rdf:type owl:Class ;
             rdfs:subClassOf :Race ;
             rdfs:isDefinedBy <http://purl.org/athletics#> ;
             rdfs:label "Race Walking"@en ;
             skos:broader :Race ;
             skos:definition "Racewalking, or race walking, is a long-distance discipline where one foot must appear to be in contact with the ground at all times."@en .


###  http://purl.org/athletics#RecordOrBest
:RecordOrBest rdf:type owl:Class ;
              dct:description "Record or Best is an athlete's best performance according to specific criteria set by a Sports Governing Body. For instance, World Records are ratified by the IAAF."@en ;
              rdfs:isDefinedBy <http://purl.org/athletics#> ;
              rdfs:label "Record or Best"@en .


###  http://purl.org/athletics#Result
:Result rdf:type owl:Class ;
        rdfs:isDefinedBy <http://purl.org/athletics#> ;
        rdfs:label "Result"@en ;
        skos:definition "Result is a competitor's achievent before (start list), during and after the competition. Result items will include information about the impact of the performance in the competition (i.e., records, disqualifications, competition ‘under protest’, etc.)."@en .


###  http://purl.org/athletics#RoadRunning
:RoadRunning rdf:type owl:Class ;
             rdfs:subClassOf :Race ;
             rdfs:isDefinedBy <http://purl.org/athletics#> ;
             rdfs:label "Road Running"@en ;
             skos:broader :Race ;
             skos:definition "Road running is an Athletics discipline on a measured course over an established road."@en .


###  http://purl.org/athletics#ShotPut
:ShotPut rdf:type owl:Class ;
         rdfs:subClassOf :Throws ;
         rdfs:isDefinedBy <http://purl.org/athletics#> ;
         rdfs:label "Shot Put"@en ;
         skos:definition "Shot Put is a track and field event involving throwing/putting (throwing in a pushing motion) a heavy spherical object (the shot) as far as possible. The size of the ball, and the legth of the wire varies depending on the competition rules."@en .


###  http://purl.org/athletics#SportsDiscipline
:SportsDiscipline rdf:type owl:Class ;
                  rdfs:subClassOf skos:Concept ;
                  rdfs:isDefinedBy <http://purl.org/athletics#> ;
                  rdfs:label "Sports Discipline"@en ;
                  skos:definition "Type of sports"@en .


###  http://purl.org/athletics#SportsGoverningBody
:SportsGoverningBody rdf:type owl:Class ;
                     rdfs:subClassOf schema:SportsOrganization ,
                                     <http://xmlns.com/foaf/0.1/Organization> ;
                     rdfs:isDefinedBy <http://purl.org/athletics#> ;
                     rdfs:label "Sports Governing Body"@en ;
                     skos:definition "Sports Governing Body is an organization in charge of governing and rule sports. These sports governing bodies may be attached to other higher-level organizations (clubs or federations) attached to them."@en .


###  http://purl.org/athletics#Sprints
:Sprints rdf:type owl:Class ;
         rdfs:subClassOf :Race ;
         rdfs:isDefinedBy <http://purl.org/athletics#> ;
         rdfs:label "Sprints"@en ;
         skos:broader :Race ;
         skos:definition "Sprints are running events over a short distance (less than 800m)."@en .


###  http://purl.org/athletics#Steeplechase
:Steeplechase rdf:type owl:Class ;
              rdfs:subClassOf :Race ;
              rdfs:isDefinedBy <http://purl.org/athletics#> ;
              rdfs:label "Steeplechase"@en ;
              skos:broader :MiddleOrLongDistanceRace ;
              skos:definition "Steeplechase is a long distance running event where runners must clear obstacles and water jumps."@en .


###  http://purl.org/athletics#Throws
:Throws rdf:type owl:Class ;
        rdfs:subClassOf :AthleticsDiscipline ;
        rdfs:isDefinedBy <http://purl.org/athletics#> ;
        rdfs:label "Throws"@en ;
        skos:broader :AthleticsDiscipline ;
        skos:definition "Throws are types of field events in which athletes throws heavy objects in an attempt to mark a farther distance than their competitors."@en ;
        skos:narrower :DiscusThrow ,
                      :HammerThrow ,
                      :JavelinThrow ,
                      :ShotPut ,
                      :WeightThrow .


###  http://purl.org/athletics#ThrowsPentathlon
:ThrowsPentathlon rdf:type owl:Class ;
                  rdfs:subClassOf :CombinedEvent ;
                  rdfs:isDefinedBy <http://purl.org/athletics#> ;
                  rdfs:label "Throws Pentathlon"@en ;
                  skos:broader :CombinedEvent ;
                  skos:definition "Throws Pentathlon is a competition in which athletes participate in five combined throwing events, earning points for their performance in each event, which adds to a total points score."@en .


###  http://purl.org/athletics#TimePerformance
:TimePerformance rdf:type owl:Class ;
                 rdfs:subClassOf :Performance ;
                 rdfs:isDefinedBy <http://purl.org/athletics#> ;
                 rdfs:label "Time Performance"@en ;
                 skos:definition "Performance measured as time"@en .


###  http://purl.org/athletics#Timekeeping
:Timekeeping rdf:type owl:Class ;
             rdfs:isDefinedBy <http://purl.org/athletics#> ;
             rdfs:label "Timekeeping"@en ;
             skos:definition "Timekeeping is a process and/or mechanism of timing competitions."@en .


###  http://purl.org/athletics#TrackRelays
:TrackRelays rdf:type owl:Class ;
             rdfs:subClassOf :Race ;
             rdfs:isDefinedBy <http://purl.org/athletics#> ;
             rdfs:label "Track Relays"@en ;
             skos:broader :Race ;
             skos:definition "Relays or relay race is a track race between two or more teams where each member of the team runs one leg of the race."@en .


###  http://purl.org/athletics#TripleJump
:TripleJump rdf:type owl:Class ;
            rdfs:subClassOf :HorizontalJumps ;
            rdfs:isDefinedBy <http://purl.org/athletics#> ;
            rdfs:label "Triple Jump"@en ;
            skos:definition "Triple Jump (sometimes referred to as the hop, step and jump or the hop, skip and jump), is a track and field event, where competitors run down the track and perform a hop, a bound and then a jump into the sand pit."@en .


###  http://purl.org/athletics#UltraRunning
:UltraRunning rdf:type owl:Class ;
              rdfs:subClassOf :Race ;
              rdfs:isDefinedBy <http://purl.org/athletics#> ;
              rdfs:label "Ultra Running"@en ;
              skos:broader :Race ;
              skos:definition "Ultra running, ultra distance or ultramarathon, is a footrace longer than the traditional marathon length (42.195 Km)."@en .


###  http://purl.org/athletics#UnitCompetition
:UnitCompetition rdf:type owl:Class ;
                 rdfs:subClassOf :Competition ;
                 dct:description "A Unit Competition is a unitary competition defined by specific sports rules (for instance, the Heat 1 race at the semifinals of 100m event in the Olympic Games). Competition Rounds have one or more Unit Competitions."@en ;
                 rdfs:isDefinedBy <http://purl.org/athletics#> ;
                 rdfs:label "Unit Competition"@en .


###  http://purl.org/athletics#UnitDistanceCompetition
:UnitDistanceCompetition rdf:type owl:Class ;
                         rdfs:subClassOf :UnitFieldCompetition ;
                         rdfs:isDefinedBy <http://purl.org/athletics#> ;
                         rdfs:label "Unit Distance Competition"@en ;
                         skos:definition "A Unit Distance Competition is a unitary competition for distance field disciplines (Horizontal Jumps and Throws)."@en .


###  http://purl.org/athletics#UnitFieldCompetition
:UnitFieldCompetition rdf:type owl:Class ;
                      rdfs:subClassOf :UnitCompetition ;
                      rdfs:isDefinedBy <http://purl.org/athletics#> ;
                      rdfs:label "Unit Field Competition"@en ;
                      skos:definition "A Unit Field Competition is a unitary competition in Athletics field events (for instance, one group of the semifinals in a High Jump event)."@en .


###  http://purl.org/athletics#UnitHeightCompetition
:UnitHeightCompetition rdf:type owl:Class ;
                       rdfs:subClassOf :UnitFieldCompetition ;
                       rdfs:isDefinedBy <http://purl.org/athletics#> ;
                       rdfs:label "Unit Height Competition"@en ;
                       skos:definition "A Unit Height Competition is a unitary competition for height disciplines (Vertical Jumps)."@en .


###  http://purl.org/athletics#UnitRace
:UnitRace rdf:type owl:Class ;
          rdfs:subClassOf :UnitCompetition ;
          rdfs:isDefinedBy <http://purl.org/athletics#> ;
          rdfs:label "Unit Race"@en ;
          skos:definition "A Unit Race is a competitive event where performances are measured as time (i.e., races and legs in relay races). Timed events have specific information about timekeeping. This kind of events may have information about the start/finish point and the course of the race."@en .


###  http://purl.org/athletics#VerticalJumps
:VerticalJumps rdf:type owl:Class ;
               rdfs:subClassOf :AthleticsDiscipline ;
               rdfs:isDefinedBy <http://purl.org/athletics#> ;
               rdfs:label "Vertical Jumps"@en ;
               skos:definition "Vertical Jumps are track and field events in which competitors must jump or vault over a horizontal bar placed at measured heights without dislodging it."@en ;
               skos:narrower :HighJump ,
                             :PoleVault .


###  http://purl.org/athletics#WeightThrow
:WeightThrow rdf:type owl:Class ;
             rdfs:subClassOf :Throws ;
             rdfs:isDefinedBy <http://purl.org/athletics#> ;
             rdfs:label "Weight Throw"@en ;
             skos:broader :Throws ;
             skos:definition "Weight Throw is a track and field event usually play indoors which is equivalent to the hammer throw event, which can only be held outdoors. Ball weight may vary depending on the rules of the competition."@en .


