#################################################################
#    Object Properties
#################################################################

###  http://purl.org/athletics#athlete
:athlete rdf:type owl:ObjectProperty ;
         rdfs:range :Athlete ;
         rdfs:isDefinedBy <http://purl.org/athletics#> ;
         rdfs:label "athlete"@en ;
         skos:definition "A person that takes part in athletics competitions. The can act as member of a sports team or sports club."@en .


###  http://purl.org/athletics#bestPerformance
:bestPerformance rdf:type owl:ObjectProperty ;
                 rdfs:domain :CompetitionAction ;
                 rdfs:range :Performance ;
                 skos:definition "Best performance achieved by an athlete or team for the discipline related to the competition."@en .


###  http://purl.org/athletics#captain
:captain rdf:type owl:ObjectProperty ;
         owl:inverseOf :captainOf ;
         rdfs:domain schema:SportsTeam ;
         rdfs:range :Athlete ;
         rdfs:isDefinedBy <http://purl.org/athletics#> ;
         rdfs:label "captain"@en ;
         skos:definition "Athlete leading a sports team."@en .


###  http://purl.org/athletics#captainOf
:captainOf rdf:type owl:ObjectProperty ;
           rdfs:domain :Athlete ;
           rdfs:range schema:SportsTeam ;
           rdfs:isDefinedBy <http://purl.org/athletics#> ;
           rdfs:label "captain of"@en ;
           skos:definition "Sports team that the athlete is captain of."@en .


###  http://purl.org/athletics#category
:category rdf:type owl:ObjectProperty ;
          rdfs:range :CompetitiveAudience ;
          rdfs:isDefinedBy <http://purl.org/athletics#> ;
          rdfs:label "category"@en ;
          skos:definition "Category of the competition based on the gender and age of the competitors who are elegible to compete."@en .


###  http://purl.org/athletics#competitionFeature
:competitionFeature rdf:type owl:ObjectProperty ;
                    rdfs:range skos:Concept ;
                    rdfs:isDefinedBy <http://purl.org/athletics#> ;
                    rdfs:label "competition feature"@en ;
                    skos:definition "Competition Feature is every note included by officials in the starting list or results (e.g., 'Qualified without standard in field events', 'Advanced to next round by Referee'). Values for this property should be part of the official standard abbreviations and symbols."@en .


###  http://purl.org/athletics#competitor
:competitor rdf:type owl:ObjectProperty ;
            rdfs:range :CompetitionAction ;
            rdfs:isDefinedBy <http://purl.org/athletics#> ;
            rdfs:label "competitor"@en ;
            skos:definition "Entry of a competitor (person or organization) taking part in a competition."@en .


###  http://purl.org/athletics#discusDiameter
:discusDiameter rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf schema:distance ;
                rdfs:domain :DiscusThrow ;
                rdfs:isDefinedBy <http://purl.org/athletics#> ;
                rdfs:label "discus diameter"@en ;
                skos:definition "Diameter of a throwing discus"@en .


###  http://purl.org/athletics#division
:division rdf:type owl:ObjectProperty ;
          rdfs:range :Competition ;
          rdfs:isDefinedBy <http://purl.org/athletics#> ;
          rdfs:label "division"@en ;
          skos:definition "Division of a Multi Division Competition."@en .


###  http://purl.org/athletics#hammerLength
:hammerLength rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf schema:distance ;
              rdfs:domain :HammerThrow ;
              rdfs:isDefinedBy <http://purl.org/athletics#> ;
              rdfs:label "hammer lenght"@en ;
              skos:definition "Lenght of the hammer wire"@en .


###  http://purl.org/athletics#hurdlesHeight
:hurdlesHeight rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf schema:height ;
               rdfs:domain :Hurdles ;
               rdfs:isDefinedBy <http://purl.org/athletics#> ;
               rdfs:label "hurdhles height"@en ;
               skos:definition "Height of hurdles"@en .


###  http://purl.org/athletics#hurdlesSpacing
:hurdlesSpacing rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf schema:distance ;
                rdfs:domain :Hurdles ;
                rdfs:isDefinedBy <http://purl.org/athletics#> ;
                rdfs:label "hurdles spacing"@en ;
                skos:definition "Space between two hurdles"@en .


###  http://purl.org/athletics#javelinLength
:javelinLength rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf schema:distance ;
               rdfs:domain :JavelinThrow ;
               rdfs:isDefinedBy <http://purl.org/athletics#> ;
               rdfs:label "javelin lenght"@en ;
               skos:definition "Lenght of the throwing javelin"@en .


###  http://purl.org/athletics#maxDuration
:maxDuration rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf schema:duration ;
             rdfs:domain :Race ;
             rdfs:isDefinedBy <http://purl.org/athletics#> ;
             rdfs:label "max duration"@en ;
             skos:definition "Maximum duration of the race in case of being a time-fixed competition (i.e., one hour, one day, etc.)."@en .


###  http://purl.org/athletics#obstaclesHeight
:obstaclesHeight rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf schema:height ;
                 rdfs:domain :Steeplechase ;
                 rdfs:isDefinedBy <http://purl.org/athletics#> ;
                 rdfs:label "obstacles height"@en ;
                 skos:definition "Height of obstacles."@en .


###  http://purl.org/athletics#performance
:performance rdf:type owl:ObjectProperty ;
             rdfs:range :Performance ;
             rdfs:isDefinedBy <http://purl.org/athletics#> ;
             rdfs:label "performance"@en ;
             skos:definition "Measure to quantify the performance of the competitor after a competition."@en .


###  http://purl.org/athletics#protestStatus
:protestStatus rdf:type owl:ObjectProperty ;
               rdfs:range skos:Concept ;
               rdfs:isDefinedBy <http://purl.org/athletics#> ;
               rdfs:label "protest status"@en ;
               skos:definition "Status of a protest action of a competitor within a competition."@en .


###  http://purl.org/athletics#qualificationCriteria
:qualificationCriteria rdf:type owl:ObjectProperty ;
                       rdfs:domain :CompetitionRound ;
                       rdfs:range :QualificationCriteria ;
                       rdfs:isDefinedBy <http://purl.org/athletics#> ;
                       rdfs:label "qualification criteria"@en ;
                       skos:definition "Qualification Criteria are the details what a competitor has to do to get to the next round in a competition."@en .


###  http://purl.org/athletics#raceCompetitionType
:raceCompetitionType rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf dct:type ;
                     rdfs:domain :Race ;
                     rdfs:range skos:Concept ;
                     rdfs:isDefinedBy <http://purl.org/athletics#> ;
                     rdfs:label "race competition type"@en ;
                     skos:definition "Type of competition in the race (classic, time trial, relays, etc.)."@en .


###  http://purl.org/athletics#raceCourse
:raceCourse rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf schema:exerciseCourse ;
            rdfs:domain :UnitRace ;
            rdfs:isDefinedBy <http://purl.org/athletics#> ;
            rdfs:label "race course"@en ;
            skos:definition "Course of a race."@en .


###  http://purl.org/athletics#recognizingAuthority
:recognizingAuthority rdf:type owl:ObjectProperty ;
                      rdfs:domain :CompetitiveAudience ;
                      rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
                      rdfs:isDefinedBy <http://purl.org/athletics#> ;
                      rdfs:label "recognizing authority"@en ;
                      skos:definition "Governing body or organization that recognizes and rule a specific category."@en .


###  http://purl.org/athletics#recordOrBest
:recordOrBest rdf:type owl:ObjectProperty ;
              rdfs:range skos:Concept ;
              rdfs:isDefinedBy <http://purl.org/athletics#> ;
              rdfs:label "record or best"@en ;
              skos:definition "A record or best is the best result that has ever been achieved in a particular sport or activity, for example the fastest time, the furthest distance, or the greatest number of victories."@en .


###  http://purl.org/athletics#results
:results rdf:type owl:ObjectProperty ;
         rdfs:range rdf:Seq ;
         rdfs:isDefinedBy <http://purl.org/athletics#> ;
         rdfs:label "results"@en ;
         skos:definition "Results of a competition. The list of results —competitors and their performances."@en .


###  http://purl.org/athletics#round
:round rdf:type owl:ObjectProperty ;
       rdfs:range :CompetitionRound ;
       rdfs:isDefinedBy <http://purl.org/athletics#> ;
       rdfs:label "round"@en ;
       skos:definition "Round part of a Multi Round Competition."@en .


###  http://purl.org/athletics#sportsDiscipline
:sportsDiscipline rdf:type owl:ObjectProperty ;
                  rdfs:range :SportsDiscipline ;
                  rdfs:isDefinedBy <http://purl.org/athletics#> ;
                  rdfs:label "sports discipline"@en ;
                  skos:definition "Type of an Athletics competition according to specific rules set by governing bodies (e.g., Outdoor Sprint Relays)."@en .


###  http://purl.org/athletics#stage
:stage rdf:type owl:ObjectProperty ;
       rdfs:range :Competition ;
       rdfs:isDefinedBy <http://purl.org/athletics#> ;
       rdfs:label "stage"@en ;
       skos:definition "Competition stage in a Multi Stage Competition."@en .


###  http://purl.org/athletics#startingHeight
:startingHeight rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf schema:height ;
                rdfs:domain :UnitHeightCompetition ;
                rdfs:isDefinedBy <http://purl.org/athletics#> ;
                rdfs:label "starting height"@en ;
                skos:definition "The starting height the bar is raised at the start of a vertical jumps unit competition."@en .


###  http://purl.org/athletics#targetHeight
:targetHeight rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf schema:height ;
              rdfs:isDefinedBy <http://purl.org/athletics#> ;
              rdfs:label "target height"@en ;
              skos:definition "The target height the bar is raised during a round of attempts."@en .


###  http://purl.org/athletics#timekeeping
:timekeeping rdf:type owl:ObjectProperty ;
             rdfs:domain :UnitRace ;
             rdfs:range :Timekeeping ;
             rdfs:isDefinedBy <http://purl.org/athletics#> ;
             rdfs:label "timekeeping"@en ;
             skos:definition "Timekeeping system used in a race."@en .


###  http://purl.org/athletics#timekeepingType
:timekeepingType rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf dct:type ;
                 rdfs:domain :Timekeeping ;
                 rdfs:range skos:Concept ;
                 rdfs:isDefinedBy <http://purl.org/athletics#> ;
                 rdfs:label "timekeeping type"@en ;
                 skos:definition "Type of timekeeping method used (FAT, HT, TST)."@en .


###  http://purl.org/athletics#trackAndFieldVenueType
:trackAndFieldVenueType rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf dct:type ;
                        rdfs:domain :Race ;
                        rdfs:range skos:Concept ;
                        rdfs:isDefinedBy <http://purl.org/athletics#> ;
                        rdfs:label "track and field venue type"@en ;
                        skos:definition "Type of venue that holds Athletics competitions (indoors or outdoors)."@en .


###  http://purl.org/athletics#unitCompetition
:unitCompetition rdf:type owl:ObjectProperty ;
                 rdfs:domain :Competition ;
                 rdfs:range :UnitCompetition ;
                 rdfs:isDefinedBy <http://purl.org/athletics#> ;
                 rdfs:label "unit competition"@en ;
                 skos:definition "Unitary sports competition that is part of a competition."@en .


###  http://purl.org/athletics#windAssistance
:windAssistance rdf:type owl:ObjectProperty ;
                rdfs:domain :TimePerformance ;
                rdfs:isDefinedBy <http://purl.org/athletics#> ;
                rdfs:label "wind assistance"@en ;
                skos:definition "Wind assistance. In track and field, wind assistance is the benefit that an athlete receives during a race or event as registered by a wind gauge. Wind speed is usually measured in meters per second (m/s), and the wind assistance can be expressed with either positive or negative values."@en .


