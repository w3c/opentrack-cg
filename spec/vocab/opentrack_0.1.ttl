@prefix : <http://purl.org/athletics#> .
@prefix bbc: <http://www.bbc.co.uk/ontologies/sport/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix schema: <http://schema.org/> .
@base <http://purl.org/athletics#> .

<http://purl.org/athletics#> rdf:type owl:Ontology ;
                              owl:versionIRI <http://purl.org/athletics/0.1#> ;
                              dct:description "The OpenTrack (&Field) Vocabulary defines a set of concepts and terms for describing competitions in Athletics. This vocabulary includes classes and terms related to competitions (events), competitors, and competition management (results, participation, etc.)."@en ;
                              dct:issued "2017-06-10"^^xsd:date ;
                              dct:creator "Martin Alvarez-Espinar" ;
                              dct:license <https://www.w3.org/community/about/agreements/final/> ;
                              <http://purl.org/vocab/vann/preferredNamespacePrefix> "ath" ;
                              rdfs:label "Opentrack Ontology"@en ;
                              owl:versionInfo "0.1" ;
                              dct:contributor "W3C Opentrack Community Group" ;
                              rdfs:comment "This is the RDF ontology for Opentrack version 0.1 (working draft)."@en ;
                              <http://purl.org/vocab/vann/preferredNamespaceUri> <http://purl.org/athletics#> ;
                              <http://purl.org/vocab/vann/example> :Athlete .

